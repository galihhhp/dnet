{
  "name": "dnet-app-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "json-server --watch database.json --port 3000",
    "dev:all": "concurrently \"bun run dev\" \"bun run server\"",
    "start": "json-server --watch database.json --port $PORT --host 0.0.0.0 --static ./dist"
  },
  "dependencies": {
    "@ant-design/charts": "^2.6.0",
    "@ant-design/icons": "^5.3.7",
    "antd": "^5.17.0",
    "axios": "^1.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.6.3",
    "xlsx": "^0.18.5",
    "json-server": "^0.17.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^6.0.0"
  }
}